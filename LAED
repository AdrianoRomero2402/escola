#include<stdio.h>
int matriz[3] [3] ,i ,j ,n=3, acum;
float prom;

main(){
	for(i=0;i<n;i++){
		for(j=0;j<n;j++){
			printf("ingrese valor :");
			scanf("%d", &matriz [i][j]);
				}
			}
			for(i=0;i<n;i++){
				for(j=0;j<n;i++){
					printf(matriz[i][j]);
				}
			printf("\n");
			}
	for(i=0;i<n;i++){
		for(j=0;j<n;j++){
			acum=acum+matriz[i][j];
		}

			printf("\n");
		prom = acum /3;	
			printf ("el promedio de cada alumno es %f", prom);
	acum=0;
	prom=0;		
	}
	for(j=0;j<n;j++){
		for(i=0;i<n;i++){
			acum=acum+matriz[i][j];
		}

			printf("\n");
		prom = acum /3;	
			printf ("el promedio de cada prueba es %f", prom);
	acum=0;
	prom=0;		
	}
	

}
